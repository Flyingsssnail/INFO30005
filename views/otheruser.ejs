

<head>
    <link rel = "stylesheet" href="othercss.css">
    <meta charset="UTF-8">

    <title>Title</title>
</head>

<body>
<div class="container">
    <br><br><br><br>
    <div class="row cf">
        <div id="card1" class="card four col">
            <div class="image-wrapper"></div>
            <h3 class="name">
                <% result.forEach(function(element) { %>
                    <%= element.name%>
                <% })%>
            </h3>
            <div class="info cf">
                <div class="four col"><span class="number">
                        <% result.forEach(function(element) { %>
                            <%= element.post%>
                        <% })%>
                    </span>Posts</div>
                <div class="four col"><span class="number">
                        <% result.forEach(function(element) { %>
                            <%= element.collect%>
                        <% })%>
                    </span>Collects</div>
                <div class="four col"><span class="number">
                        <% result.forEach(function(element) { %>
                            <%= element.likes%>
                        <% })%>
                    </span>Likes</div>
            </div>
            <br><br>
            <div class="info cf">
                <div class="four col"><span class="number">
                        <% result.forEach(function(element) { %>
                            <%= element.following%>
                        <% })%>
                    </span>Following</div>
                <div class="four col"><span class="number">
                        <% result.forEach(function(element) { %>
                            <%= element.followers%>
                        <% })%>
                    </span>Followers</div>
                <div class="four col"><span class="number">
                        <% result.forEach(function(element) { %>
                            <%= element.liked%>
                        <% })%>
                    </span>Liked</div>
            </div>
        </div>

        <div id="card2" class="card four col">
            <div class="header"></div>
            <div class="wrapper">
                <div class="image-wrapper"></div>
                <h3 class="name">About me</h3>

                <div class="info">
                    <p>This man is lazy, leaving nothing behind...</p>
                </div>
            </div>
        </div>

        <div id="card1" class="card four col">
            <div class="header" style="background-color: #0d76be; border-radius: 8px;"><a href="/profile/edit" style="text-decoration: none; color: white;">EDIT</a></div>
            <div class="wrapper">
                <h2 class="name">More about me...</h2>
            </div>
            <div class="options">
                <ul>
                    <li><span class="icon"><i class="fa fa-plus" ></i></span>Rank:
                        <% result.forEach(function(element) { %>
                            <%= element.rank%>
                        <% })%></li>
                    <li><span class="icon"><i class="fa fa-envelope" ></i></span>Level:
                        <% result.forEach(function(element) { %>
                            <%= element.exp%>
                        <% })%></li>
                    <li><span class="icon"><i class="fa fa-envelope" ></i></span>Gender:
                        <% result.forEach(function(element) { %>
                            <%= element.gender%>
                        <% })%></li>
                    <li><span class="icon"><i class="fa fa-plus" ></i></span>Phone:
                        <% result.forEach(function(element) { %>
                            <%= element.phone%>
                        <% })%></li>
                    <li><span class="icon"><i class="fa fa-envelope" ></i></span>E-mail:
                        <% result.forEach(function(element) { %>
                            <%= element.email%>
                        <% })%></li>
                    <li><span class="icon"><i class="fa fa-envelope" ></i></span>BD:
                        <% result.forEach(function(element) { %>
                            <%= element.BD%>
                        <% })%></li>

                </ul>
            </div>
        </div>
    </div>
</div>
</div>
</body>
<script src="otheruser.js"></script>
</html>