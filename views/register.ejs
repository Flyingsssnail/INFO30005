<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Register page</title>
    <link rel="stylesheet" type="text/css" href="../public/register.css">
</head>
<body>
     <div class="container">
         <form class="well_form-horizontal" action=" " method="post"  id="register_page">

             <fieldset>

                 <!-- Form Name -->
                 <h1>Welcome to join us</h1>

                 <br>

                 <!-- Text input-->
                 <div class="form-group">
                     <label class="col-md-4 control-label">Name</label>
                     <div class="col-md-4 inputGroupContainer">
                         <div class="input-group">
                             <% result.forEach(function(element) { %>
                                 <%= element.name%>
                             <% })%>
                         </div>
                     </div>
                 </div>


                 <div class="form-group">
                     <label class="col-md-4 control-label">First Name</label>
                     <div class="col-md-4 inputGroupContainer">
                         <div class="input-group">
                             <% result.forEach(function(element) { %>
                                 <%= element.firstname%>
                             <% })%>
                         </div>
                     </div>
                 </div>

                 <!-- Text input-->

                 <div class="form-group">
                     <label class="col-md-4 control-label" >Last Name</label>
                     <div class="col-md-4 inputGroupContainer">
                         <div class="input-group">
                             <% result.forEach(function(element) { %>
                                 <%= element.lastname%>
                             <% })%>
                         </div>
                     </div>
                 </div>

                 <!-- Text input-->
                 <div class="form-group">
                     <label class="col-md-4 control-label">E-Mail</label>
                     <div class="col-md-4 inputGroupContainer">
                         <div class="input-group">
                             <% result.forEach(function(element) { %>
                                 <%= element.email%>
                             <% })%>
                         </div>
                     </div>
                 </div>

                 <div class="form-group">
                     <label class="col-md-4 control-label">Password</label>
                     <div class="col-md-4 inputGroupContainer">
                         <div class="input-group">
                             <% result.forEach(function(element) { %>
                                 <%= element.password%>
                             <% })%>

                         </div>
                     </div>
                 </div> <!-- close .form-group -->

                 <div class="form-group">
                     <label class="col-md-4 control-label"> Confirm Password </label>
                     <div class="col-md-4 inputGroupContainer">
                         <div class="input-group">
                             <% result.forEach(function(element) { %>
                                 <%= element.password%>
                             <% })%>
                         </div>
                     </div>
                 </div>



                 <div class="form-group" >
                     <label class="col-md-4 control-label">Gender</label>
                     <div class="col-md-4" >
                         <div class="radio">
                             <label>
                                 <input type="radio" name="gender" value="male" /> Male
                             </label>
                         </div>
                         <div class="radio">
                             <label>
                                 <input type="radio" name="gender" value="female" /> Female
                             </label>
                         </div>
                         <div class="radio">
                             <label>
                                 <input type="radio" name="gender" value="other" /> Other
                             </label>
                         </div>
                         <div class="radio">
                             <label>
                                 <input type="radio" name="gender" value="hidden" /> Hidden
                             </label>
                         </div>
                     </div>
                 </div>

                 <!-- Button -->
                 <div class="form-group">
                     <label class="col-md-4 control-label"></label>
                     <div class="col-md-4">
                         <div style="text-align: center;"><button type="submit" class="btn" >Submit </button></div>
                     </div>
                 </div>
             </fieldset>
         </form>
     </div>
</body>
</html>