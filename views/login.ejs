<html>
<head>
    <link rel = "stylesheet" href="login.css" type="text/css">
</head>
<body>



<div class ="login" >
    <h1 id ="app" class = "login-header">
        <% result.forEach(function(element) { %>
        <%= element.name%>
        <% })%>
    </h1>
</div>
<br>
<main class ="login-form">


    <h3 id = "username">
    {{message}}</h3>
    <input id = "user" type="text" placeholder="Username"/><br>
    <h3 id = "password" >
        {{message}}</h3>
    <input id = "pass" type="password" placeholder="Password"/><br>
<br>
    <input type="button" value="Login" onclick="wronguser()" class="login-button"/>
    <br>

    <a class="help" href="../public/register.html">Sign up!</a><br>
    <br>
    <a href="../public/forget.html" class="help">forget your password?</a>
    <br>
    <br>

</main>
<br>

<script src="../public/login.js"></script>
</body>
</html>